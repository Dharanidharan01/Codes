describe('AliExpress Search Automation - College Project', () => {
  it('Should search for iPhone on AliExpress', () => {
    // Step 1: Visit AliExpress US homepage
    cy.visit('https://www.aliexpress.us/?gatewayAdapt=glo2usa')

    // Step 2: Type "iPhone" in the search bar and press Enter
    cy.get('input[placeholder="Search"]').type('iPhone{enter}')

    // Step 3: Wait for results to load and confirm
    cy.url().should('include', 'search')
    cy.contains('iPhone').should('exist')
  })
})
